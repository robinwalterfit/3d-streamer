    "license": "MIT",
    "name": "3d-streamer",
    "packageManager": "pnpm@7.10.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/robinwalterfit/3d-streamer.git"
    },
    "scripts": {
        "cy:component:open": "cypress open --component",
        "cy:component:run": "cypress run --component",
        "cy:component:run:headless": "cypress run --component --headless",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "cy:run:headless": "cypress run --headless",
        "dev": "NODE_OPTIONS='--inspect' next dev",
        "export": "pnpm run generate && next export",
        "generate": "ANALYZE=true next build",
        "lint": "pnpm run prettier:check && pnpm run lint:eslint",
        "lint:eslint": "eslint . --ext graphql,js,jsx,ts,tsx --max-warnings=0 --config ./.eslintrc --no-eslintrc",
        "lint:staged": "lint-staged",
        "prepare": "husky install",
        "prettier:check": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "serve": "next start",
        "start": "pnpm run dev",
        "test": "pnpm run test:component && pnpm run test:e2e",
        "test:component": "pnpm run cy:component:run:headless",
        "test:e2e": "start-server-and-test serve http://localhost:3000 cy:run:headless",
        "typecheck": "tsc -p tsconfig.json"
    },
    "version": "0.1.0"
}
